<UserControl x:Class="GarageAdministration.WPF.Views.Home.HomeView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:GarageAdministration.WPF.Views.Home"
             xmlns:home="clr-namespace:GarageAdministration.WPF.ViewModels.Home"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <Button Content="{Binding RandomText}" 
                Command="{Binding NavigateToCreateOwnerViewCommand}"></Button>
    </Grid>
</UserControl>
